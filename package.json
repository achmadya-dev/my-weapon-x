{
  "name": "my-weapon-x",
  "version": "1.0.0",
  "description": "a boilerplate for build api with ExpressJs",
  "main": "server.ts",
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/server.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "build": "tsc",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "npm run lint",
      "npm run prettier"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.4",
    "eslint": "^9.11.1",
    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5",
    "typescript-eslint": "^8.8.0"
  },
  "dependencies": {
    "express": "^4.21.0"
  }
}
